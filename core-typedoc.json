{
  "tsconfig": "tsconfig.json",
  "entryPoints": ["../core/src/"],
  "basePath": "../",
  "exclude": ["../core/src/util/node-fetch/*", "**/*.spec.ts", "**/*.spec.tsx"],
  "entryPointStrategy": "expand",
  "useTsLinkResolution": true,
  "name": "Lawtext core references",
  "plugin": ["typedoc-plugin-markdown"],
  "cleanOutputDir": true,
  "readme": "none",
  "fileExtension": ".mdx",
  "membersWithOwnFile": ["Class", "Interface"],
  "out": "pages/lib"
}
